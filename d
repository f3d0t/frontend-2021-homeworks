[32m+[m[32mconst man = new Human('Tom', 'male', 'Hello Jenny!', ['Tom', 'Jenny', 'Rex', 'Selina']),[m[41m[m
[32m+[m[32m  women = new Human('Jenny', 'female', 'Hello Tom!', ['Tom', 'Rex', 'Felix']),[m[41m[m
[32m+[m[32m  dog = new Dog('Rex', 'male', 'woof-woof!', ['Tom', 'Jenny']),[m[41m[m
