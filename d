[32m+[m[32m// ======== OBJECTS DEFINITIONS ========[m[41m[m
[32m+[m[32m// Define your objects here[m[41m[m
[32m+[m[32mclass Resident {[m[41m[m
[32m+[m[32m  constructor(species, name, gender, saying, friends) {[m[41m[m
[32m+[m[32m    this.species = species;[m[41m[m
[32m+[m[32m    this.name = name;[m[41m[m
[32m+[m[32m    this.gender = gender;[m[41m[m
[32m+[m[32m    this.friends = friends;[m[41m[m
[32m+[m[32m    this.saying = saying;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  toString() {[m[41m[m
[32m+[m[32m    return `Wonderful creature - <b>${this.species}</b>,[m[41m [m
[32m+[m[32m      whose name is <b>${this.name}</b>,[m[41m [m
[32m+[m[32m      the gender is <b>${this.gender}</b>![m[41m [m
[32m+[m[32m      Usually the greeting is <b>${this.saying}</b>[m[41m [m
[32m+[m[32m      The friends of this inhabitant are <b>${this.friends.join(', ')}</b>. `;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mclass Human extends Resident {[m[41m[m
[32m+[m[32m  constructor(name, gender, saying, friends, legs = 2, hands = 2) {[m[41m[m
[32m+[m[32m    super('human', name, gender, saying, friends);[m[41m[m
[32m+[m[32m    this.legs = legs;[m[41m[m
[32m+[m[32m    this.hands = hands;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  toString() {[m[41m[m
[32m+[m[32m    return ([m[41m[m
[32m+[m[32m      super.toString() +[m[41m[m
[32m+[m[32m      `By the wat, this member is owner of <b>${this.legs} legs</b> and <b>${this.hands} hands</b>!`[m[41m[m
[32m+[m[32m    );[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mclass Animal extends Resident {[m[41m[m
[32m+[m[32m  constructor(species, name, gender, saying, friends, paws = 4) {[m[41m[m
[32m+[m[32m    super(species, name, gender, saying, friends);[m[41m[m
[32m+[m[32m    this.paws = paws;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  toString() {[m[41m[m
[32m+[m[32m    return ([m[41m[m
[32m+[m[32m      super.toString() +[m[41m[m
[32m+[m[32m      `By the wat, this member is owner of <By>${this.paws} paws</By>!`[m[41m[m
[32m+[m[32m    );[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mclass Dog extends Animal {[m[41m[m
[32m+[m[32m  constructor(name, gender, saying, friends) {[m[41m[m
[32m+[m[32m    super('dog', name, gender, saying, friends);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mclass Cat extends Animal {[m[41m[m
[32m+[m[32m  constructor(name, gender, saying, friends) {[m[41m[m
[32m+[m[32m    super('cat', name, gender, saying, friends);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mclass CatWoman extends Human {[m[41m[m
[32m+[m[32m  constructor(name, friends = ['Tom', 'Felix']) {[m[41m[m
[32m+[m[32m    super(name, 'female', CatWoman.saying(), friends);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  static saying() {[m[41m[m
[32m+[m[32m    return 'meow!';[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
